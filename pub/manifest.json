{
	"manifest_version": 2,
	"name": "temp",
	"version": "0.0.0",
	"description": "temp",
	"permissions": [
		"tabs",
		"webRequest",
		"*://*/*"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"content_scripts": [
		{
		  "matches": ["http://*/*", "https://*/*"],
		  "js": ["js/contentscript.js"],
		  "all_frames": true,
		  "run_at": "document_start"
		}
	],
	//"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
	"browser_action": {
		"default_icon": "icon/icon.png"
	}
}